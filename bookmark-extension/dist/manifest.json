{
  "manifest_version": 3,
  "name": "Bookmark Extensions",
  "version": "1.0.0",
  "description": "使用回数に基づいた整理および自動削除機能を備えたChromeブックマークマネージャ",
  "permissions": ["bookmarks", "storage", "tabs", "alarms"],
  "action": {
    "default_title": "Click!",
    "default_icon": { "16": "icon.png" }
  },
  "background": {
    "service_worker": "background.js"
  }
}
