{
  "manifest_version": 3,
  "name": "Bookmark Extensions",
  "version": "1.0.0",
  "description": "使用回数・期限管理および削除提案機能を備えたブックマークマネージャ",
  "permissions": ["bookmarks", "storage", "tabs"],
  "action": {
    "default_title": "Click!",
    "default_icon": { "16": "icon.png" }
  },
  "background": {
    "service_worker": "background.js"
  }
}
